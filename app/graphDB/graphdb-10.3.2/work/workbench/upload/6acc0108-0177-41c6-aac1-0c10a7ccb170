@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.example.org/> .

<http://www.example.org/> rdf:type owl:Ontology ;
      owl:versionIRI <http://www.example.org/2024-04-09> .

#################################################################
#    Object Properties
#################################################################

### http://www.example.org/evidence
<http://www.example.org/evidence> rdf:type owl:ObjectProperty ;
                                    rdfs:comment "The evidence used to support a claim" ;
                                    rdfs:label "evidence" .

#################################################################
#    Datatype Properties
#################################################################

### http://www.example.org/qualifier
<http://www.example.org/qualifier> rdf:type owl:ObjectProperty ;
                                    rdfs:comment "A qualifier associated with an entity" ;
                                    rdfs:label "qualifier" .

### http://www.example.org/beginning
<http://www.example.org/beginning> rdf:type owl:ObjectProperty ;
                                    rdfs:comment "The starting point of a temporal period" ;
                                    rdfs:label "beginning" .

### http://www.example.org/end
<http://www.example.org/end> rdf:type owl:ObjectProperty ;
                              rdfs:comment "The ending point of a temporal period" ;
                              rdfs:label "end" .

### http://www.example.org/hasLatitude
<http://www.example.org/hasLatitude> rdf:type owl:ObjectProperty ;
                                      rdfs:comment "The latitude coordinate of a location" ;
                                      rdfs:label "latitude" .

